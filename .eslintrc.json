{
    "extends": "airbnb-base",
    "plugins": [
        "import",        
        "html"
    ],
    "rules": {
        "import/newline-after-import": "off",
        "linebreak-style": "off",
        "no-trailing-spaces": "off",
        "no-tabs": "off",
        "spaced-comment": "warn",
        "no-undef": 1,
        "indent":"off",
        "brace-style": "off",
        "no-var": "off",
        "vars-on-top": "off",
        "space-before-function-paren": "off",
        "no-shadow": "off",
        "func-names": "off",
        "no-unused-vars": "off",
        "quotes": "off",
        "no-plusplus": "off",
        "prefer-destructuring": "off",
        "no-loop-func": "off",
        "max-len": "off",
        "no-param-reassign": "off"

    }
}