<mxEditor defaultGroup="group" defaultEdge="edge"
	helpWindowImage="images/help.gif"
	tasksWindowImage="images/tasks.gif"
	forcedInserting="0"
	swimlaneRequired="0">
	<include name="config/editor-commons.xml"/>
	<Array as="cycleAttributeValues">
		<add value="#83027F"/>
		<add value="#66B922"/>
		<add value="#808913"/>
		<add value="#CF0056"/>
		<add value="#4679B6"/>
	</Array>
	<Array as="templates">
		<add as="pattern">
			<Pattern name="Pattern">
				<mxCell vertex="1" style="swimlane" connectable="0" isSwimlane="1">
					<mxGeometry as="geometry" width="220" height="480"/>
				</mxCell>
			</Pattern>
		</add>
		<add as="parameter">
			<Parameter name="Parameter" type="">
				<mxCell vertex="1" style="fontSize=9;shape=ellipse;resizable=0;" port="1">		
					<mxGeometry as="geometry" width="32" height="32"/>
				</mxCell>
			</Parameter>
		</add>
		<add as="patternBody">
			<PatternBody name="PatternBody" description="" href="">
				<mxCell vertex="1" style="fontSize=9;" connectable="0" port="2">		
					<mxGeometry as="geometry" width="200" height="60"/>
				</mxCell>
			</PatternBody>
		</add>		
		<add as="pathexpression">
			<PathExpression name="PathExpression" type="">
				<mxCell edge="1" style="pathexpression">		
					<mxGeometry as="geometry" isRelative="1"/>
				</mxCell>
			</PathExpression>
		</add>
		<add as="compare">
			<Compare name="Compare">
				<mxCell vertex="1" style="compare">		
					<mxGeometry as="geometry" width="60" height="50"/>
				</mxCell>
			</Compare>
		</add>
		<add as="check">
			<Check name="Check">
				<mxCell vertex="1" style="check">		
					<mxGeometry as="geometry" width="60" height="50"/>
				</mxCell>
			</Check>
		</add>
		<add as="patterncomposition">
			<PatternComposition name="PatternComposition">
				<mxCell vertex="1" style="patterncomposition">		
					<mxGeometry as="geometry" width="60" height="50"/>
				</mxCell>
			</PatternComposition>
		</add>

		<add as="symbol">
			<Symbol name="Symbol" description="" href="">
				<mxCell vertex="1" style="fontSize=9;shape=ellipse;resizable=0;" port="1">		
					<mxGeometry as="geometry" width="32" height="32"/>
				</mxCell>
			</Symbol>
		</add>
		<add as="group">
			<Group name="" description="">
				<mxCell vertex="1" style="group" connectable="0"/>
			</Group>
		</add>
		<add as="edge">
			<Edge name="" description="">
				<mxCell edge="1">
					<mxGeometry as="geometry" isRelative="1"/>
				</mxCell>
			</Edge>
		</add>		
		<add as="task">
			<Task name="Task">
				<mxCell vertex="1">	
					<mxGeometry as="geometry" width="80" height="30"/>
				</mxCell>
			</Task>
		</add>
		<add as="subprocess">
			<Subprocess name="Subprocess">
				<mxCell vertex="1" style="rounded">		
					<mxGeometry as="geometry" width="80" height="30"/>
				</mxCell>
			</Subprocess>
		</add>
		<!-- <add as="hline">
			<Shape label="">
				<mxCell vertex="1" style="ellipse">		
					<mxGeometry as="geometry" width="60" height="10"/>
				</mxCell>
			</Shape>
		</add> -->
		<add as="variable">
			<Variable name="Variable">
				<mxCell vertex="1" style="rounded">		
					<mxGeometry as="geometry" width="80" height="30"/>
				</mxCell>
			</Variable>
		</add>		
	</Array>
	<add as="createTasks">VWQLEditorConfig.createTasks</add>
</mxEditor>
